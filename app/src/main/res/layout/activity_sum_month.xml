<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="my.ungersinc.user.shiftmanager.SumMonth"
    android:paddingBottom="16dp">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView2" >

        <GridLayout
            android:layout_height="match_parent" android:layout_width="match_parent"
            android:rowCount="18"
            android:columnCount="2">

            <Spinner
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:id="@+id/spinnerMonth"
                android:spinnerMode="dropdown"
                android:layout_gravity="center"
                android:layout_columnSpan="1"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_row="0"
                android:layout_column="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumTo"
                android:id="@+id/tillC"
                android:layout_row="1"
                android:layout_column="1"
                android:textAlignment="center"
                android:layout_gravity="center_horizontal"
                android:visibility="gone"
                android:layout_marginTop="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumFrom"
                android:id="@+id/fromC"
                android:layout_row="1"
                android:layout_column="0"
                android:textAlignment="center"
                android:layout_gravity="center_horizontal"
                android:visibility="gone"
                android:layout_marginTop="10dp" />

            <TextView

                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumWorks"
                android:id="@+id/btnWorks"
                android:layout_column="1"
                android:clickable="true"
                android:layout_gravity="center"
                android:layout_row="0"

                android:textColor="#FFFFFF"
                android:textSize="18sp"
                android:layout_width="170dp"
                android:layout_height="30dp"
                android:background="@drawable/button_works"
                android:shadowColor="#A8A8A8"
                android:shadowDx="0"
                android:shadowDy="0"
                android:shadowRadius="5"
                android:gravity="center|center_horizontal"
                android:layout_marginTop="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0/0/0"
                android:id="@+id/tillTv"
                android:layout_row="2"
                android:layout_column="0"
                android:layout_gravity="center_horizontal"
                android:background="#ff29fffa"
                android:visibility="gone" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0/0/0"
                android:id="@+id/fromTv"
                android:layout_row="2"
                android:layout_column="1"
                android:layout_gravity="center_horizontal"
                android:background="#ff29fffa"
                android:visibility="gone" />

            <TextView
                android:layout_width="140dp"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/textView6"
                android:layout_column="0"
                android:text="@string/SumSalary"
                android:layout_marginTop="20dp"
                android:layout_row="4" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/textView7"
                android:layout_column="1"
                android:text="@string/SumSalaryCash"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:layout_row="4" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="sumMonth"
                android:id="@+id/sumMonthTV"
                android:layout_column="0"
                android:layout_row="5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/sumCashMonthTV"
                android:layout_column="1"
                android:text="sumCashMonthv"
                android:layout_marginLeft="30dp"
                android:layout_row="5" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumCash"
                android:id="@+id/textView9"
                android:layout_column="0"
                android:layout_marginTop="30dp"
                android:layout_row="7" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumNumEvent"
                android:id="@+id/textView13"
                android:layout_column="1"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="30dp"
                android:layout_row="7" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="tipCash"
                android:id="@+id/tipCashTV"
                android:layout_column="0"
                android:layout_row="9" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/NumberEventsTV"
                android:layout_column="1"
                android:text="numEvents"
                android:layout_marginLeft="30dp"
                android:layout_row="9" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumDrives"
                android:id="@+id/textView8"
                android:layout_column="0"
                android:layout_marginTop="30dp"
                android:layout_row="10" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumHours"
                android:id="@+id/textViewHours"
                android:layout_column="1"
                android:layout_marginTop="30dp"
                android:layout_marginLeft="30dp"
                android:layout_row="10" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="drives"
                android:id="@+id/drivesTV"
                android:layout_column="0"
                android:layout_row="11" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="hours"
                android:id="@+id/hoursTV"
                android:layout_column="1"
                android:layout_marginLeft="30dp"
                android:layout_row="11" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumDistance"
                android:id="@+id/textView11"
                android:layout_column="0"
                android:layout_marginTop="20dp"
                android:layout_row="12" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumAverageShift"
                android:id="@+id/textView4"
                android:layout_column="1"
                android:layout_marginLeft="30dp"
                android:layout_row="12"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/distanceTV"
                android:layout_column="0"
                android:text="distace"
                android:layout_row="14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="avgEvent"
                android:id="@+id/avgEventTV"
                android:layout_column="1"
                android:layout_marginLeft="30dp"
                android:layout_row="14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumParking"
                android:id="@+id/textView12"
                android:layout_column="0"
                android:layout_marginTop="20dp"
                android:layout_row="15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="@string/SumAverageForHour"
                android:id="@+id/textViewkk"
                android:layout_column="1"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="30dp"
                android:layout_row="15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Parking:"
                android:id="@+id/parkingTV"
                android:layout_column="0"
                android:layout_row="16" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="avgHours"
                android:id="@+id/avgHoursTV"
                android:layout_column="1"
                android:layout_marginLeft="30dp"
                android:layout_row="16" />


            <Button
                android:layout_width="310dp"
                android:layout_height="wrap_content"
                android:text="@string/SumExcel"
                android:id="@+id/buttonExport"
                android:layout_column="0"
                android:nestedScrollingEnabled="false"
                android:layout_columnSpan="2"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="11dp"
                android:layout_row="17" />

        </GridLayout>
    </ScrollView>

</FrameLayout>